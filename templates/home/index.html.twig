{% extends 'base.html.twig' %}

{% block title %}Tuto symfony :{% endblock %}

{% block css %}
<link rel="stylesheet" href="{{ asset('assets/Main/main.h1.style.css') }}" />
{% endblock %}
{% block content %}
<div class="example-wrapper">
    <h1>Tuto symfony :</h1>
    {% for user in users %}
        <div>
            {{ user.name }}
        </div>
        <div>
            {% for produit in user.produits %}
                <img src="{{ produit.source }}" alt="{{ produit.alt }}"/>
            {% endfor %}
        </div>
    {% endfor %}
</div>
{% endblock %}